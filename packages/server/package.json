{
  "name": "@blog-aws-practice/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/express.ts",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run generate && tsc",
    "build:docker": "pnpm run clean && pnpm run generate && tsc",
    "generate": "pnpm run generate:prisma && pnpm run generate:codegen",
    "generate:prisma": "prisma generate",
    "generate:codegen": "pnpm run generate:codegen:prepare && pnpm run generate:codegen:concat && pnpm run generate:codegen:dts",
    "generate:codegen:prepare": "rimraf schema/schema.gql",
    "generate:codegen:concat": "concat -o schema/schema.gql schema/*.gql",
    "generate:codegen:dts": "graphql-codegen --config codegen.yml",
    "insert-seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seeds.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:up": "docker-compose -f ../../docker-compose.yml up -d",
    "docker:down": "docker-compose -f ../../docker-compose.yml down",
    "docker:logs": "docker-compose -f ../../docker-compose.yml logs -f",
    "docker:build": "docker build -t blog-aws-practice-server .",
    "seed": "tsx prisma/seeds.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-codegen/cli": "^5.0.7",
    "@prisma/client": "^6.9.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^8.1.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.9.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@types/express": "4",
    "@types/node": "15.3.0",
    "concat": "1.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.4"
  },
  "volta": {
    "node": "22.11.0"
  }
}
